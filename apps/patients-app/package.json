    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@prisma/client": "^4.14.0",
    "@tabler/icons-react": "^2.17.0",
    "@tanstack/react-query": "^4.29.3",
    "@trpc/client": "^10.23.0",
